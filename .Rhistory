convert_age <- function(age) {
if(age == '5-14 years') {
return(9.5);
} else if(age == '15-24 years') {
return(19.5);
} else if(age == '25-34 years') {
return (29.5);
} else if(age == '34-55 years') {
return (44.5);
} else if(age == '55-74 years') {
return (64.5);
} else {
return(87.5);
}
}
